{
  "name": "sfdx-example-plugin",
  "version": "0.0.0",
  "author": "Jeff Dickey @jdxcode",
  "bugs": "https://github.com/jdxcode/sfdx-example-plugin/issues",
  "dependencies": {
    "@oclif/command": "^1.3.1",
    "@oclif/config": "^1.3.56",
    "@oclif/errors": "^1.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.2.15",
    "@oclif/plugin-help": "^1.1.0",
    "@oclif/test": "^1.0.1",
    "@oclif/tslint": "^1.0.2",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "chai": "^4.1.2",
    "globby": "^8.0.0",
    "mocha": "^5.0.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib"
  ],
  "homepage": "https://github.com/jdxcode/sfdx-example-plugin",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "hello": {
        "description": "example hello topic",
        "foo": {
          "description": "example foo topic"
        }
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "jdxcode/sfdx-example-plugin",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "clean": "rm -f .oclif.manifest.json",
    "lint": "tsc -p test --noEmit && tslint -p test -t stylish",
    "postpublish": "yarn run clean",
    "posttest": "yarn run lint",
    "prepare": "yarn run prepublishOnly",
    "prepublishOnly": "yarn run build && oclif-dev manifest",
    "preversion": "yarn run clean",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
